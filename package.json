{
  "name": "the-beth-stack",
  "module": "src/index.tsx",
  "type": "module",
  "scripts": {
    "db:generate": "bunx drizzle-kit generate:sqlite",
    "db:migrate": "bunx drizzle-kit migrate:sqlite",
    "db:push": "bunx drizzle-kit push:sqlite",
    "db:studio": "bunx drizzle-kit studio",
    "dev": "concurrently \"bun run --hot src/index.tsx\" \"bun run tw:dev\"",
    "tw": "bunx tailwindcss -i ./src/styles.css -o ./tailwind-gen/styles.css",
    "tw:dev": "bunx tailwindcss -i ./src/styles.css -o ./tailwind-gen/styles.css --watch"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "latest",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "bun-types": "latest",
    "drizzle-kit": "^0.19.13",
    "tailwind-content-placeholder": "^0.0.5",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-full-bleed": "^2.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/fn": "^0.6.1",
    "@elysiajs/html": "^0.6.1",
    "@elysiajs/static": "^0.6.0",
    "@fontsource/noto-sans": "^5.0.12",
    "@grotto/logysia": "^0.0.2",
    "@libsql/client": "^0.3.4",
    "@lucia-auth/adapter-sqlite": "^2.0.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "elysia": "^0.6.22",
    "elysia-autoroutes": "^0.2.2",
    "libsql": "^0.1.11",
    "lucia": "^2.6.0"
  }
}
